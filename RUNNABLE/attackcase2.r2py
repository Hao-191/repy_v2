# read operation conditions
# if file exists, clean up
if "testfile.txt.a" in listfiles():
  removefile("testfile.txt.a")
if "testfile.txt.b" in listfiles():
  removefile("testfile.txt.b")
  
# Create an AB file
myfile = ABopenfile("testfile.txt/0", True)

# Write a string with a null byte and control character to test handling of special characters.
myfile.writeat("Stest12345E" + '\0' + '/0', 0)

# Read
myfile.readat(None, 0)

# Close the file.
myfile.close()