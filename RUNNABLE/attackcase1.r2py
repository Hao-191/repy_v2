# Clean up existing files
if "testfile.a" in listfiles():
    removefile("testfile.a")
if "testfile.b" in listfiles():
    removefile("testfile.b")

# Create a new ABFile
myfile = ABopenfile("testfile", True)

# Check that the new file contains the initial contents 'SE'
try:
    assert 'SE' == myfile.readat(2, 0), "Initial content 'SE' not found in file A"
    myfile.close()
except:
    myfile.close()
    log("Failed to create file or read initial contents 'SE'")